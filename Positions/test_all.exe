export PATH=/c/Users/Vincent/AppData/Local/Programs/Python/Python36:$PATH
for file in `ls -1 solution*.txt`
do
    echo -n '.'
    python ../classes/chess_main.py < $file > output_$file
    nbLines=`cat output_$file | grep "Echec et mat !" | wc -l`
    if [ $nbLines -ne 1 ]
    then
        echo -n 'F1'
    fi
    output_ref=`echo "output_$file" | sed 's/txt/ref/g'`
    touch $output_ref
    dos2unix $output_ref 2> /dev/null
    dos2unix output_$file 2> /dev/null
    nbLines=`diff  output_$file $output_ref | wc -l`
    if [ $nbLines -ne 0 ]
    then
        mv output_$file $output_ref
        echo -n 'F2'
    else
        rm output_$file
    fi
done
